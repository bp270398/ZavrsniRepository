        "SmtpServer": "smtp.gmail.com",
        "Port": 587
    },
    "ConnectionStrings": {
        "RhetosConnectionString": "Data Source=SQL_SERVER;Initial Catalog=DATABASE_NAME;Integrated Security=SSPI;"
    }
