            "Microsoft.AspNetCore": "Warning"
        },
        "Console": {
            "LogLevel": {
                //"ImpersonationService": "Trace",
                //"AuthorizationManager": "Trace",
                //"CommonAuthorizationProvider": "Trace"
            }
        }
